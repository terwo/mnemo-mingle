{
    "manifest_version": 3,
    "name": "MnemoMingle",
    "description": "Make new vocabulary more meaningful",
    "version": "1.0",
    "icons": {
        "128": "images/icon.png"
    },
    "permissions": [
        "activeTab"
],
    "commands": {
        "highlight-text": {
          "suggested_key": {
            "default": "Q"
          },
          "description": "Highlight text and show popup"
        }
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png"
    }
}